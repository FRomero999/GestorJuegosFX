<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.gestorjuegosfx.MainController">
   <center>
      <VBox alignment="CENTER" spacing="20.0">
         <children>
            <ListView fx:id="lista" prefHeight="200.0" prefWidth="200.0" />
            <TableView fx:id="tabla" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="cEmail" prefWidth="75.0" text="Email" />
                <TableColumn fx:id="cContraseña" prefWidth="75.0" text="Contraseña" />
              </columns>
            </TableView>
            <ChoiceBox fx:id="choice" prefWidth="150.0" />
            <Button fx:id="btnSalir" mnemonicParsing="false" onAction="#salir" text="Salir" />
         </children>
      </VBox>
   </center>
</BorderPane>
